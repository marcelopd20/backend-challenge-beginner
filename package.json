{
  "name": "backend-challenge-begginer",
  "version": "1.0.0",
  "description": "This project allows consulting assets(stocks, exchange, cyrpto) using Yahoo Finance API by customers, and create a list preferences",
  "main": "app.js",
  "repository": "https://github.com/marcelopd20/backend-challenge-beginner/",
  "author": "Marcelo Pastana Duarte <marcelopd20@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.prod.json && tsc-alias",
    "lint": "eslint ./src ./src/tests --ext .ts",
    "lint:fix": "eslint ./src ./src/tests --ext .ts --fix",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --respawn --transpile-only --ignore-watch node_modules --no-notify src/app.ts",
    "test": "jest --coverage --detectOpenHandles --no-cache",
    "start": "node -r ts-node ./.build/src/app.js"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongodb": "^4.13.0"
  },
  "devDependencies": {
    "@jest/types": "^28.1.1",
    "@types/express": "^4.17.14",
    "@types/jest": "^28.1.1",
    "@types/node": "^18.11.13",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "jest-mock-extended": "^3.0.1",
    "nock": "^13.2.6",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.4"
  }
}
